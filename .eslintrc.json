{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly",
		"describe": true,
		"it": true,
		"expect": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["eslint-plugin-react"],
	"rules": {},
	"settings": {
		"react": {
			"pragma": "React",
			"version": "16.0"
		}
	},
	"parser": "babel-eslint"
}
