{
	"name": "react-starter",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-scripts": "2.1.8"
	},
	"husky": {
		"hooks": {
			"pre-commit": "eslint ./src/**/*.js",
			"pre-push": "npm run test"
		}
	},
	"scripts": {
		"prettier": "prettier --write ./src/**/*.js",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"chai": "^4.2.0",
		"eslint": "^5.16.0",
		"mocha": "^6.1.2",
		"node-sass": "^4.11.0",
		"prettier": "^1.16.4",
		"eslint-plugin-react": "^7.12.4",
		"husky": "^1.3.1"
	}
}
